{
  "name": "@js-modular-seed/app-scripts",
  "version": "0.0.1",
  "description": "Example application and consumer of this project libraries. Uses script references to serve JS",
  "main": "index.js",
    "repository": {
    "type": "git",
    "url": "git+https://github.com/christianacca/js-modular-seed.git"
  },
  "scripts": {
    "watch:debug": "node ../scripts/node_modules/webpack/bin/webpack.js --watch --env.debug",
    "watch": "node ../scripts/node_modules/webpack/bin/webpack.js --watch",
    "server": "node ../scripts/node_modules/http-server/bin/http-server -c -1 -o",
    "every-dep": "node ../scripts/node_modules/linklocal/bin/linklocal.js list | node ../scripts/node_modules/bulk/cli.js -c",
    "build": "npm run every-dep -- \"npm run build\"",
    "build:prod": "npm run every-dep -- \"npm run build:prod\"",
    "clean": "echo \"Nothing to clean\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "relink": "node ../scripts/node_modules/linklocal/bin/linklocal.js",
    "unlink": "node ../scripts/node_modules/linklocal/bin/linklocal.js unlink && npm install --production",
    "shrinkwrap:remove": "del npm-shrinkwrap.json",
    "preshrinkwrap": "npm run shrinkwrap:remove && npm run unlink",
    "postshrinkwrap": "npm run relink"
  },
  "private": true,  
  "keywords": [],
  "author": "christianacca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianacca/js-modular-seed/issues"
  },  
  "dependencies": {
    "@js-modular-seed/lib1": "0.0.2",
    "@js-modular-seed/lib2": "0.0.2",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "@js-modular-seed/lib1": "file:../lib/lib1",
    "@js-modular-seed/lib2": "file:../lib/lib2"
  }
}
