{
  "name": "@js-modular-seed/app-webpack",
  "version": "0.0.1",
  "description": "Example application and consumer of this project libraries. Uses webpack to bundle JS from both the application and library folders",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean:build",
    "build": "node ../node_modules/webpack/bin/webpack.js",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "node ../node_modules/webpack/bin/webpack.js --env.prod",
    "clean": "npm run clean:build",
    "clean:build": "node ../node_modules/shx/lib/cli.js rm -rf build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "node ../node_modules/webpack/bin/webpack.js",
    "relink": "node ../node_modules/linklocal/bin/linklocal.js",
    "unlink": "node ../node_modules/linklocal/bin/linklocal.js unlink && npm install --production",
    "preshrinkwrap": "npm run unlink && npm prune",
    "postshrinkwrap": "npm run relink"
  },
  "keywords": [],
  "author": "christianacca",
  "license": "MIT",
  "dependencies": {
    "@js-modular-seed/lib1": "^0.0.1",
    "@js-modular-seed/lib2": "^0.0.1",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "@js-modular-seed/lib1": "file:../lib/lib1",
    "@js-modular-seed/lib2": "file:../lib/lib2"
  }
}
