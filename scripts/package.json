{
  "name": "js-modular-seed",
  "version": "0.0.1",
  "description": "javascript seed project that supports a modular code base",
  "scripts": {
    "cmd": "shx ls -d ../app-*/ ../lib/*/ | bulk -c",
    "preclean:full": "npm run clean",
    "clean:full": "shx ls -d ../app-*/ ../lib/*/ | bulk -c \"shx rm -rf node_modules\"",
    "clean": "shx ls -d ../app-*/ ../lib/*/ | bulk -c \"npm run clean\"",
    "postinstall": "shx ls -d ../app-*/ ../lib/*/ | bulk -c \"npm install\" && shx ls -d ../app-*/ | bulk -c \"linklocal -r\"",
    "tmp": "shx ls -d ../app-scripts/ | bulk -c \"npm install\" && shx ls -d ../app-scripts/ | bulk -c \"linklocal -r\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "shx ls -d ../app-webpack/ ../lib/*/ | bulk -c \"webpack-validator webpack.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christianacca/js-modular-seed.git"
  },
  "keywords": [],
  "author": "christianacca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianacca/js-modular-seed/issues"
  },
  "homepage": "https://github.com/christianacca/js-modular-seed#readme",
  "dependencies": {
    "bulk": "github:kumarharsh/bulk#windows-support",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "linklocal": "github:theoy/linklocal#alwaysJunctionOnWindows",
    "node-sass": "^3.8.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "shx": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "^2.2.2"
  }
}
